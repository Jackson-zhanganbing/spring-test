<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zab.springboot.common.async.mapper.AsyncTaskMapper">

    <update id="updateByTaskNo">
        update sys_async_task set `status` = #{status} where task_no = #{taskNo}
    </update>
</mapper>
